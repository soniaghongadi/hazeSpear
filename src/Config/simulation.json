{
  "peerJS": {
    "cloudServerName": "CloudServer",
    "host": "localhost",
    "port": 9000,
    "path": "/hazespear"
  },
  "fog": {
    "number": 10,
    "perDeviceMessageRetentionSize": 1000,
    "retention": false
  },
  "sensor": {
    "number": 200,
    "delayBetweenSensorAwakeInSeconds": 60
  },
  "rabitmq": {
    "directConfig": {
      "url": "amqp://guest:guest@localhost:5672"
    },
    "rascalConfig": {
      "vhosts": {
        "v1": {
          "connection": {
            "url": "amqp://guest:guest@localhost:5672"
          }
        }
      }
    }
  }
}
